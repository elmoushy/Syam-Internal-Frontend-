# WHSO.GOV.AE Production Environment Configuration
# API Configuration
VITE_API_BASE_URL=https://whso.gov.ae:8000/api/
VITE_FRONTEND_BASE_URL=https://whso.gov.ae:5173

# Environment
VITE_ENVIRONMENT=production
VITE_DEBUG_MODE=false

# WebSocket Configuration - uses wss:// for production (secure)
VITE_WEBSOCKET_ENABLED=true
VITE_WS_URL=wss://whso.gov.ae:8000

# Security settings
VITE_MAX_LOGIN_ATTEMPTS=3
VITE_SHOW_SECURITY_WARNINGS=false
VITE_CLIENT_SIDE_VALIDATION=true

VITE_INPUT_MAX_LENGTHS={"title":255,"description":1000,"question":500,"option":200}
